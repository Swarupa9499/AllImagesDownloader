{
  "manifest_version": 3,
  "name": "Image Downloader with Timestamp Folders",
  "version": "1.1",
  "description": "Download all images from a webpage into timestamped folders",
  "permissions": ["activeTab", "downloads", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}